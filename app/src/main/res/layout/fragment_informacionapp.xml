<?xml version="1.0" encoding="utf-8"?>
<LinearLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:orientation="vertical"
    tools:context="fragmentsPackage.quejasysugenencias"
    android:background="@android:color/white"
    android:weightSum="3"
    android:layout_margin="16dp"
    android:paddingTop="20dp"
    android:paddingBottom="20dp">
    <!-- TODO: Update blank fragment layout -->


    <ImageView
        android:id="@+id/imageViewcorreo"
        android:layout_width="match_parent"
        android:layout_height="0dp"
        android:layout_marginTop="10dp"
        android:layout_weight="0.50"
        android:layout_marginStart="10dp"
        android:layout_marginEnd="10dp"
        android:src="@drawable/icon_contact_orange" />

    <TextView
        android:layout_width="match_parent"
        android:layout_height="0dp"
        android:layout_marginTop="10dp"
        android:layout_weight="0.35"
        android:layout_marginStart="90dp"
        android:layout_marginEnd="10dp"
        android:text="LUNCHAPPWO@GMAIL.COM"
        android:textColor="#ff6634"
        />
    <ImageView
        android:id="@+id/imageViewequipo"
        android:layout_width="match_parent"
        android:layout_height="0dp"
        android:layout_marginTop="10dp"
        android:layout_weight="0.50"
        android:layout_marginStart="10dp"
        android:layout_marginEnd="10dp"
        android:src="@drawable/icon_equipo_orange" />

    <TextView
        android:id="@+id/vicentename"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginStart="90dp"
        android:layout_marginTop="10dp"
        android:layout_marginEnd="10dp"
        android:layout_weight="0.10"
        android:text="VICENE AGUILERA PÉREZ"
        android:textColor="#ff6634"
    />
    <TextView
        android:id="@+id/victorname"
        android:layout_width="wrap_content"
        android:layout_height="6dp"
        android:layout_marginStart="100dp"
        android:layout_marginTop="10dp"
        android:layout_marginEnd="10dp"
        android:layout_weight="0.10"
        android:text="PÉREZ ESPINO VICTOR"
        android:textColor="#ff6634"
        />
    <TextView
    android:id="@+id/omarname"
    android:layout_width="223dp"
    android:layout_height="6dp"
    android:layout_marginStart="70dp"
    android:layout_marginTop="10dp"
    android:layout_marginEnd="10dp"
    android:layout_weight="0.10"
    android:text="CASTILLO ZAMUDIO OMAR JALIL"
    android:textColor="#ff6634"
    />




</LinearLayout>